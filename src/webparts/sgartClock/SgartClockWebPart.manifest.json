{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "f26ec826-c4ee-4373-8674-eab892fc2708",
  "alias": "SgartClockWebPart",
  "componentType": "WebPart",
  // The "*" signifies that the version should be taken from the package.json
  "version": "*",
  "manifestVersion": 2,
  // If true, the component can only be installed on sites where Custom Script is allowed.
  // Components that allow authors to embed arbitrary script code should set this to true.
  // https://support.office.com/en-us/article/Turn-scripting-capabilities-on-or-off-1f2c515f-5d7e-448a-9fd7-835da935584f
  "requiresCustomScript": false,
  "supportedHosts": [
    "SharePointWebPart",
    "TeamsPersonalApp",
    "TeamsTab",
    "SharePointFullPage"
  ],
  "supportsThemeVariants": true,
  "preconfiguredEntries": [
    {
      "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Advanced
      "group": {
        "default": "Advanced"
      },
      "title": {
        "default": "Sgart Clock"
      },
      "description": {
        "default": "Clock web part (Sgart.it)"
      },
      "officeFabricIconFontName": "Clock",
      "properties": {
        "webpartTitle": "Sgart Clock",
        "size": 0,
        "showHandSeconds": true,
        "backgroundColor": "#EEE",
        "borderColor": "#000",
        "showNotches": "Minutes1",
        "notches1Color": "#000",
        "notches5Color": "#000",
        "notches15Color": "#000",
        "handHoursColor": "rgba(51,51,51,.95)",
        "handMinutesColor": "rgba(51,51,51,.95)",
        "handSecondsColor": "rgba(255,51,51,.95)",
        "handPointColor": "rgba(51,51,51,.99)",
        "clockTextReorder": "Foreground",
        "clockText": "sgart.it",
        "clockTextColor": "#800",
        "digitalClockReorder": "Foreground",
        "showDigitalClockSeconds": true,
        "digitalClockColor": "#888"
      }
    }
  ]
}